<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="local.Car">
    <property name="speed" type="d" access="read"/>
    <property name="rpm" type="d" access="read"/>
    <property name="battery" type="d" access="read"/>
    <signal name="speedChanged">
    </signal>
    <signal name="rpmChanged">
    </signal>
    <signal name="batteryChanged">
    </signal>
    <signal name="canFail">
    </signal>
    <signal name="canRestored">
    </signal>
    <method name="setSpeed">
      <arg name="speed" type="d" direction="in"/>
    </method>
    <method name="setRPM">
      <arg name="rpm" type="d" direction="in"/>
    </method>
    <method name="setBattery">
      <arg name="battery" type="d" direction="in"/>
    </method>
    <method name="canDisconnected">
    </method>
    <method name="checkCanRestored">
    </method>
    <method name="getSpeed">
      <arg type="d" direction="out"/>
    </method>
    <method name="getRPM">
      <arg type="d" direction="out"/>
    </method>
    <method name="getBattery">
      <arg type="d" direction="out"/>
    </method>
  </interface>
</node>
